u672c \u96b8\u5c6c \u7684 \u5718\u9ad4 \u90fd \u662f \u65b0 \u5718 \u9ad4 \u88cf \u9762 \u7684 \u8d85 \u7d1a \u5c0f \u773e \u53ef\u662f \u65b0\u7d44\u7e54 \u90fd \u9858\u610f \u670d\u5f9e \u5728 \u4ed6 \u7684 \u9818\u5c0e \u4e4b\u4e0b \u9019 \u4e5f \u662f \u70ba \u4ec0\u9ebc \u5373\u4f7f \u6b66\u660c \u9769\u547d \u6210\u529f \u4e86 \u4eba \u5728 \u570b\u5916 \u770b \u5831 \u7d19 \u624d \u77e5\u9053 \u7684 \u5b6b \u5927 \u7832 \u6703 \u88ab \u7b2c\u4e00 \u6642\u9593 \u8acb \u56de\u4f86 \u76f4\u63a5 \u7576 \u5927 \u7e3d\u7d71 \u4e8b\u5be6\u4e0a \u9019 \u4e5f \u662f \u570b\u6c11 \u9769\u547d \u8ddf \u4e2d\u570b \u6b77\u53f2 \u4e0a \u7684 \u4efb\u4f55 \u4e00\u5834 \u6539\u671d\u63db\u4ee3 \u7684 \u95dc\u9375 \u4e0d\u540c \u9ede \u6b77\u53f2 \u4e0a \u7684 \u4efb\u4f55 \u4e00\u5834 \u6539\u671d\u63db\u4ee3 \u90fd \u662f \u7531 \u6253\u4e0b \u5929\u4e0b \u7684 \u4eba \u7576 \u8001\u5927 \u4e8b\u5be6\u4e0a \u9769\u547d \u6210\u529f \u5f8c \u771f\u6b63 \u638c\u6b0a \u90fd \u662f \u5730\u65b9 \u4e0a\u6709 \u5be6\u529b \u4eba\u7269 \u57fa\u672c\u4e0a \u5c31 \u8ecd\u982d \u50cf \u5c71\u897f \u95bb\u932b\u5c71 \u5f9e \u7576\u5e74 \u8f9b\u4ea5\u9769\u547d \u7387 \u65b0\u8ecd \u8d77\u4e8b \u6210\u70ba \u8f9b\u4ea5\u9769\u547d \u6700\u5317 \u5730 \u8d77\u4e8b \u7701\u4efd \u7136 \u5f8c \u4ed6 \u5c31 \u5be6\u8cea \u63a7\u5236 \u5c71\u897f \u4e00\u76f4 \u5230 \u6c11\u570b 38 \u5e74 \u6c5f\u5c71 \u6613\u5e5f \u96f2\u5357 \u5247 \u662f \u6ec7 \u7cfb \u8ecd\u95a5 \u7576 \u653f \u5f9e \u8521\u9354 \u5510\u7e7c\u582f \u5230 \u9f8d\u96f2 \u76e7\u6f22 ... \u56db\u5ddd \u5247 \u662f \u9769\u547d \u8d77\u4e8b \u6210\u529f \u5f8c \u7531 \u8ecd\u95a5 \u5efa\u7acb \u5927\u6f22 \u56db\u5ddd \u8ecd \u653f\u5e9c \u5f62\u6210 \u7368\u7acb \u738b\u570b \u4e5f \u56e0\u70ba \u5982\u6b64 \u51fa\u73fe \u5404\u5730 \u9769\u547d \u8ecd \u6253 \u4e0d\u540c \u65d7\u5e5f \u65bc \u662f \u958b\u59cb \u5354\u8abf \u6700 \u5f8c \u7531 \u6c5f\u6d59 \u5730\u5340 \u4e3b\u6d41 \u4e94\u8272\u65d7 \u52dd\u51fa \u6c11\u570b \u653f\u6cbb \u4e2d\u5fc3 \u5728 \u5357\u4eac \u6210\u70ba \u65b0 \u6210\u7acb \u7684 \u4e2d\u83ef\u6c11\u570b\u570b\u65d7 \u5b6b\u6587 \u5c6c\u610f \u7684 \u9752\u5929\u767d\u65e5 \u6eff\u5730 \u7d05\u65d7 \u7576\u6642 \u9078\u70ba \u6d77\u8ecd \u65d7 \u800c \u6210\u70ba \u570b\u65d7 \u8981 \u5230 \u8523\u516c \u5317\u4f10 \u7d71\u4e00 \u5f8c \u5728 \u5148\u7e3d\u7d71 \u8523\u516c \u4e4b\u524d \u570b\u6c11\u9ee8 \u9ad4\u7cfb \u70ba \u9996 \u7684 \u4e2d\u6a1e \u5f9e\u4f86 \u4e0d\u662f \u771f\u7684 \u6709\u5927 \u5be6\u6b0a \u4f46\u662f \u7562\u7adf \u99ac \u4e0a\u5f97 \u5929\u4e0b \u4e0d\u80fd \u99ac \u4e0a \u6cbb \u5929\u4e0b \u7576\u4e86 \u7687\u5e1d \u7684 \u8001\u5927 \u9084\u662f \u8981 \u8fc5\u901f \u5efa\u7acb \u6587\u5b98 \u9ad4\u5236 \u627e \u4e1e\u76f8 \u5e6b \u4ed6 \u60f3 \u600e\u9ebc \u6cbb\u7406 \u5929\u4e0b \u7684 \u554f\u984c \u4f46\u662f \u570b\u6c11 \u9769\u547d \u537b \u662f \u76f8\u53cd \u6253\u4e0b \u5929\u4e0b \u7684 \u56de\u982d \u53bb \u5949 \u672a\u4f86 \u8981 \u6cbb\u7406 \u5929\u4e0b \u7684 \u7576 \u8001\u5927 \u800c \u4e0d\u662f \u81ea\u5df1 \u7576 \u8001\u5927 \u88ab \u8acb \u56de\u4f86 \u7576 \u8001\u5927 \u7684 \u5b6b \u5927 \u7832 \u9084 \u9032 \u4e00\u6b65 \u7684 \u70ba\u4e86 \u6cb3\u87f9 \u7518\u9858 \u8b93 \u7d66 \u8881\u4e16\u51f1 \u7576 \u8001\u5927 \u4e8b\u5be6\u662f \u7576\u6642 \u5317\u6d0b \u8ecd \u7cbe\u92b3 \u5df2\u7d93 \u5357\u4e0b \u9769\u547d \u8ecd\u5be6\u529b \u4e0d\u6575 \u8981 \u77e5\u9053 \u5b6b\u6587\u4efb \u81e8\u6642\u5927 \u7e3d\u7d71 \u6642 \u9769\u547d \u767c \u6e90\u5730 \u6b66\u660c \u5df2\u7d93 \u88ab \u5317\u6d0b \u8ecd\u596a\u4e0b \u4e26\u975e \u5728 \u9769\u547d \u8ecd \u624b\u4e0a \u800c \u897f\u6d0b \u5217\u5f37 \u52e2\u529b \u4e5f \u652f\u6301 \u8881\u4e16\u51f1 \u8881\u638c \u5317\u6d0b \u8ecd \u5927\u6b0a \u53c8 \u6709 \u591a\u5e74 \u653f\u6cbb \u5916\u4ea4 \u5be6\u52d9 \u7d93\u9a57 \u8207 \u5176 \u8b93 \u70cf\u5408 \u4e4b \u773e \u53c8 \u7f3a\u4e4f \u653f\u6cbb \u5be6\u52d9 \u7d93\u9a57 \u7684 \u9769\u547d \u9ee8\u4eba \u7576 \u653f \u8b93 \u8881\u4e16\u51f1 \u7d71\u6cbb \u4e2d\u570b \u662f \u7406\u60f3 \u5f88\u591a \u7684 \u9078\u64c7 \u5b6b\u6587 \u4e0d \u8d70 \u4eba \u4e5f \u4e0d\u884c \u4e0d \u53ef\u5426 \u8a8d \u7684 \u662f \u5982\u679c \u4e0d\u662f \u5b6b \u5927 \u7832 \u807d \u8d77 \u4f86 \u5f88 \u597d \u807d \u6709 \u5922 \u6700\u7f8e \u7684 \u4e09 \u6c11\u4e3b \u7fa9 \u6c11\u4e3b \u5171\u548c \u9084 \u6709 \u4ed6 \u6700 \u8d77 \u78bc \u4ee5 \u516c\u958b \u4f86\u8aaa \u5927\u516c \u7121\u79c1 \u7684 \u884c\u70ba \u51dd\u805a \u4e86 \u5168\u570b \u6975\u5927 \u7684 \u5411\u5fc3\u529b \u5426\u5247 \u7576\u6642 \u7684 \u4e2d\u570b \u4e00\u5b9a \u6703 \u843d\u5f97 \u50cf \u6b77\u53f2 \u4e0a \u4efb\u4f55 \u4e00\u500b \u671d\u4ee3 \u7684 \u672b\u671f \u4e00\u6a23 \u5982\u679c \u4e0d \u9000 \u4e5f \u5c31 \u9769\u547d \u8ecd \u88ab \u6253\u5f97 \u5c41\u6efe\u5c3f\u6d41 \u4e8c\u6b21\u9769\u547d \u5c31 \u5982\u6b64 \u7136 \u5f8c \u8881\u4e16\u51f1 \u6210\u70ba \u5e73\u5b9a \u5929\u4e0b \u4e4b \u5927 \u529f\u81e3 \u958b \u5927\u6e05 \u4e4b \u65b0\u5c40 \u4e00\u5806 \u8ecd\u95a5 \u5927\u5bb6 \u6253\u4f86 \u6253\u53bb \u6253 \u4e86 \u5e7e \u5341\u5e74 \u751a\u81f3 \u4e0a\u767e\u5e74 \u4ee5 \u5f8c \u6700 \u5f8c \u90a3\u500b \u6700\u5f37 \u7684 \u7d71\u4e00 \u5929\u4e0b \u4e8b\u5be6\u4e0a \u78ba\u5be6 \u5b6b \u4e2d\u5c71 \u548c \u8881\u4e16\u51f1 \u6b7b \u5f8c \u4e2d\u570b \u5c31 \u9677\u5165 \u4e86 \u8ecd\u95a5 \u5272\u64da \u4e86 \u4e00\u76f4 \u5230 \u6253\u8457 \u5b6b \u4e2d\u5c71 \u601d\u60f3 \u7cbe\u795e \u7e7c\u627f \u4eba \u7684 \u8523 \u4e2d\u6b63 \u51fa\u4f86 \u624d \u91cd\u65b0 \u7d71\u4e00 \u5929\u4e0b \u9769\u547d \u96d6\u7136 \u8981 \u6253 \u7684 \u662f \u6b66\u529b \u4f46\u662f \u771f\u6b63 \u5728 \u6253 \u7684 \u5176 \u5be6\u662f \u4eba\u5fc3 \u6c92\u6709 \u5805\u5f37 \u7684 \u7cbe\u795e \u865f \u53ec\u529b \u6700 \u5f8c \u4e5f \u4e0d \u904e \u5c31\u662f \u50cf \u500b \u660e\u671d \u5be7\u738b \u7279\u5225 \u7684 \u662f \u4e2d\u570b \u6b77\u53f2 \u4e0a \u641e \u9019\u7a2e \u865f \u53ec\u529b \u53ea\u6709 \u5169\u62db \u4e00\u7a2e \u53eb\u4f5c \u524d\u671d \u7121\u9053 \u4e0a\u5929 \u53eb \u6211 \u4f86 \u53d6\u800c\u4ee3\u4e4b \u4e00 \u662f \u4f9d\u9760 \u5b97\u6559 \u65b9\u6cd5 \u84bc\u5929 \u4ee5 \u6b7b \u9ec3\u5929 \u7576\u7acb \u5b6b \u4e2d\u5c71 \u662f \u7b2c\u4e00 \u500b \u4ee5 \u65b0 \u7684 \u653f\u6cbb \u9ad4\u5236 \u5927\u516c \u7121\u79c1 \u7684 \u65b0 \u4e2d\u570b \u70ba \u865f \u53ec \u6210\u529f \u7684 \u9769\u547d \u5b6b\u6587 \u9769\u547d \u6e05\u671d \u7f75 \u591a \u5147 \u4e00\u6a23 \u662f \u8d70 \u524d\u671d \u7121 \u9053\u8def \u7dda \u4e3b\u7fa9 \u662f \u601d\u60f3 \u662f \u4fe1\u4ef0 \u662f \u529b\u91cf \u4e00\u6a23 \u641e \u5b97\u6559 \u65b9\u6cd5 \u96d6\u7136 \u4e2d\u9593 \u7684 \u904e\u7a0b \u78ba\u5be6 \u5f97\u5230 \u975e\u5e38 \u591a \u4eba \u7684 \u5e6b\u52a9 \u5149\u9760 \u4ed6 \u672c\u4eba \u516b\u767e\u5e74 \u4e5f \u4e0d \u53ef\u80fd \u4f46\u662f \u570b\u6c11 \u9769\u547d \u78ba\u5be6 \u662f \u524d \u7121 \u53e4\u4eba \u5f15\u8ff0 Vicky1016 \u871c\u96ea \u4e4b \u9298 \u8a00 \u6b66\u660c \u8d77\u7fa9 \u80fd \u6210\u529f \u5f88 \u91cd\u8981 \u7684 \u95dc\u9375 \u662f \u5e36\u982d \u8d77\u7fa9 \u662f \u65b0\u8ecd \u65b0\u8ecd \u4ee3\u8868 \u4ec0\u9ebc \u4ee3\u8868 \u6b66\u529b \u69cd \u687f\u5b50 \u800c\u4e14 \u662f \u53d7 \u904e \u6b63\u898f \u65b0\u5f0f \u8ecd\u4e8b \u8a13\u7df4 \u7684 \u69cd\u687f\u5b50 \u70ba \u4ec0\u9ebc \u97d3\u570b \u7fa4\u773e \u6297\u722d \u90fd \u53ef\u4ee5 \u9019\u9ebc \u6709 \u7d44\u7e54 \u9019\u9ebc \u66b4\u529b \u5f88 \u91cd\u8981 \u4e00\u9ede \u97d3\u570b \u7537\u6027 \u5e7e\u4e4e \u90fd \u8981 \u7576 \u5175 \u4e00\u7576 \u4e0d\u662f \u5169\u5e74 \u5c31\u662f \u4e09\u5e74 \u6240\u4ee5 \u97d3\u570b \u7fa4\u773e \u6297\u722d \u53ef\u4ee5 \u5feb\u901f \u904b\u4f5c \u7576\u5175 \u77e5\u8b58 \u5efa\u7acb \u8ecd\u4e8b\u5316 \u7684 \u6230\u9b25 \u7d44\u7e54 \u55ae\u4f4d \u4e26 \u5be6\u884c \u6709\u6548 \u6307\u63ee\u865f \u4ee4 \u5f8c \u5099 \u8ecd\u4eba \u